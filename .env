# ==============================================================================
# PLUGIN SETTINGS
# ==============================================================================

# Navnet på utvidelsen slik det vises i WordPress Plugins-listen.
# Default: "My Plugin"
VITE_PLUGIN_NAME=

# URL-vennlig ID. Brukes til slugs, script-handles og filnavn.
# Default: "my-plugin"
VITE_PLUGIN_SLUG=

# Versjonsnummer for utvidelsen. Brukes til cache-busting av JS/CSS.
# Default: "1.0.0"
VITE_PLUGIN_VERSION=

# Beskrivelse som vises i utvidelses-oversikten.
# Default: "No description provided."
VITE_PLUGIN_DESCRIPTION=

# URL til utvidelsens hjemmeside.
# Default: ""
VITE_PLUGIN_URI=""

# Navn på utvikler/forfatter.
# Default: ""
VITE_PLUGIN_AUTHOR=""

# URL til utviklerens nettsted.
# Default: ""
VITE_PLUGIN_AUTHOR_URI=""

# Text domain for oversettelser (f.eks. i __() funksjoner).
# Default: "my-plugin"
VITE_PLUGIN_TEXT_DOMAIN=

# Relativ sti til språkfiler.
# Default: "/languages"
VITE_PLUGIN_DOMAIN_PATH=


# ==============================================================================
# MENU / PAGE SETTINGS
# ==============================================================================

# Navnet som vises i admin-menyen i WordPress.
# Default: "my-plugin-menu"
VITE_PLUGIN_MENU_NAME=

# Parent slug hvis dette skal være en undermeny (f.eks. 'options-general.php').
# Default: null
VITE_PLUGIN_PARENT_MENU_SLUG=

# Påkrevd tilgangsnivå for å se menyen.
# Default: "manage_options"
VITE_PLUGIN_MENU_CAPABILITY=

# Posisjon i menyen (viktig for toppnivå-menyer).
# Default: 25
VITE_PLUGIN_MENU_POSITION=


# ==============================================================================
# DEVELOPMENT / BUILD SETTINGS
# ==============================================================================

# Porten Vite dev-server kjører på.
# Default: 3000
VITE_PLUGIN_PORT=
